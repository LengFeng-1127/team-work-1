/* pages/index/index.wxss */
.search {
  display: flex;
  flex-direction: row;
  background-color: #f5f5f5;
  height: 52rpx;
  align-items: center;
  border-radius: 50rpx;
}
.search_item{
  display: flex;
  flex-direction: row;
  width: 90%;
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.search_item image {
  height: 42rpx;
  width: 42rpx;
  align-self: center;
  margin-left: 35%;
  margin-right: 20rpx;
}
.search_item input {
  flex: 1;
  font-size: 23rpx;
}
.test {
  display: flex;
  flex-direction: row;
  margin-top: 10%;
  margin-left: 15%;
}
.itest {
  display: flex;
  flex-direction: row;
}
.img {
  margin-left: 20%;
  background-color: #f0fafb;
}
.btnImg {
  width: 70rpx;
  height: 70rpx;
}
.btn {
  display: flex;
  flex-direction: row;
  margin-top: 40rpx;
  margin-left: 20%;
}
.btn_c {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.btn1 {
  width: 90rpx;
  height: 90rpx;
  margin: 35rpx;
  background-color: #f3f1f1;
  color: white;
  border-radius: 999rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  
}
button::after{
  border-radius: 999rpx;
  border: none;
}

.btnText{
  color: #656565;
  font-size: 28rpx;
}